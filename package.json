{
  "name": "bollettini",
  "version": "1.0.0",
  "description": "Generatore di bollettini",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "clean-build": "rimraf build dist backend",
    "build-backend": "npm run clean-build && pyinstaller --noconfirm backend.spec --distpath ./backend && copy config.ini backend\\backend",
    "dist": "npm run build-backend && electron-packager . generatore-bollettini --platform=win32 --arch=x64 --out=release-builds --overwrite --extra-resource=./backend",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "electron": "^31.2.1",
    "electron-packager": "^17.1.2",
    "rimraf": "^6.0.1"
  }
}
